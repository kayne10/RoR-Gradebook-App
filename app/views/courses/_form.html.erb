<div class="col-md-6">
  <%= form_for(@course) do |f| %>

    <%= f.label 'Teacher ID' %>
    <%= f.number_field :user_id, class: "form-control" %>

    <%= f.label :title %>
    <%= f.text_field :title, class: "form-control" %>

    <%= f.label :start_date %>
    <%= f.datetime_field :start_date, class: "form-control" %>

    <%= f.label :end_date %>
    <%= f.datetime_field :end_date, class: "form-control" %>

    <%= f.label :semester_id %>
    <%= f.number_field :semester_id, class: "form-control" %>

    <%= f.submit class: "btn btn-primary" %>
  <% end %>
  <%= link_to 'Back', current_user  %>
</div>
